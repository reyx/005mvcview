@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/arquivosrecebidos.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Arquivos Recebidos</legend>
    @Html.Hidden("ID_ARQR")
        <div class="control-group">
            <label class="control-label" for="NOME_ARQR">Nome Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.NOME_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="SITUACAO_ARQR">Situacao Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.SITUACAO_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATA_ARQR">Data Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATA_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="IP_ARQR">Ip Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.IP_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBS_ARQR">Obs Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBS_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBSCMTRBK_ARQR">Obscmtrbk Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBSCMTRBK_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="QTDEOCOR_ARQR">Qtdeocor Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.QTDEOCOR_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="SUCESSO_ARQR">Sucesso Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.SUCESSO_ARQR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="VERSAO_ARQR">Versao Arqr</label>
            <div class="controls">
                @Html.EditorFor(model => model.VERSAO_ARQR)
            </div>
        </div>
    </fieldset>
</form>
