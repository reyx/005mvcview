@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/itenskit.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Itens Kit</legend>
    @Html.Hidden("ID_ITKT")
        <div class="control-group">
            <label class="control-label" for="ID_KIT">Id Kit</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_KIT)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_PROD">Cod Prod</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_PROD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="PERCCUSTO_KIT">Perccusto Kit</label>
            <div class="controls">
                @Html.EditorFor(model => model.PERCCUSTO_KIT)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="QUANTIDADE_KIT">Quantidade Kit</label>
            <div class="controls">
                @Html.EditorFor(model => model.QUANTIDADE_KIT)
            </div>
        </div>
    </fieldset>
</form>
