@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/lacres.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Lacres</legend>
    @Html.Hidden("ID_LACRE")
        <div class="control-group">
            <label class="control-label" for="NUMERO_LACRE">Numero Lacre</label>
            <div class="controls">
                @Html.EditorFor(model => model.NUMERO_LACRE)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_VOL">Id Vol</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_VOL)
            </div>
        </div>
    </fieldset>
</form>
