@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/tiposparametrospdv.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Tipos Parametros Pdv</legend>
    @Html.Hidden("ID_TPPD")
        <div class="control-group">
            <label class="control-label" for="DESC_TPPD">Desc Tppd</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESC_TPPD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="TIPO_TPPD">Tipo Tppd</label>
            <div class="controls">
                @Html.EditorFor(model => model.TIPO_TPPD)
            </div>
        </div>
    </fieldset>
</form>
