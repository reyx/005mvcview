@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/adicoes.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Adicoes</legend>
    @Html.Hidden("ID_ADIC")
        <div class="control-group">
            <label class="control-label" for="NUMERO_ADIC">Numero Adic</label>
            <div class="controls">
                @Html.EditorFor(model => model.NUMERO_ADIC)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ITEM_ADIC">Item Adic</label>
            <div class="controls">
                @Html.EditorFor(model => model.ITEM_ADIC)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_FABR">Cod Fabr</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_FABR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DESCONTO_ADIC">Desconto Adic</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESCONTO_ADIC)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_DCIM">Id Dcim</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_DCIM)
            </div>
        </div>
    </fieldset>
</form>
