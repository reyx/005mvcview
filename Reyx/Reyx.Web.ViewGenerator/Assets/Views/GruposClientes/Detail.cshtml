@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/gruposclientes.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Grupos Clientes</legend>
        <div class="control-group">
            <label class="control-label" for="COD_GRCL">Cod Grcl</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_GRCL)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DESC_GRCL">Desc Grcl</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESC_GRCL)
            </div>
        </div>
    </fieldset>
</form>
