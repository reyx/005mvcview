@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/links.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Links</legend>
    @Html.Hidden("ID_LINK")
        <div class="control-group">
            <label class="control-label" for="ID_OPER">Id Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DESC_LINK">Desc Link</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESC_LINK)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="URL_LINK">Url Link</label>
            <div class="controls">
                @Html.EditorFor(model => model.URL_LINK)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ORDEM_LINK">Ordem Link</label>
            <div class="controls">
                @Html.EditorFor(model => model.ORDEM_LINK)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_FNPR">Id Fnpr</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_FNPR)
            </div>
        </div>
    </fieldset>
</form>
