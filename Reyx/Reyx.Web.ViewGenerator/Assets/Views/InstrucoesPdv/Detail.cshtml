@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/instrucoespdv.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Instrucoes Pdv</legend>
    @Html.Hidden("ID_INPD")
        <div class="control-group">
            <label class="control-label" for="ID_PDV">Id Pdv</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_PDV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_TAB">Id Tab</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_TAB)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="INSTR_INPD">Instr Inpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.INSTR_INPD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="VALCHAVE_INPD">Valchave Inpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.VALCHAVE_INPD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OPERACAO_INPD">Operacao Inpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.OPERACAO_INPD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATA_INPD">Data Inpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATA_INPD)
            </div>
        </div>
    </fieldset>
</form>
