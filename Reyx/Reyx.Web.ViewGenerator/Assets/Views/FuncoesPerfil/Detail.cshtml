@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/funcoesperfil.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Funcoes Perfil</legend>
    @Html.Hidden("ID_FNPF")
        <div class="control-group">
            <label class="control-label" for="ID_PERF">Id Perf</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_PERF)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_FNPR">Id Fnpr</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_FNPR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="PERMITIDO_FNPF">Permitido Fnpf</label>
            <div class="controls">
                @Html.EditorFor(model => model.PERMITIDO_FNPF)
            </div>
        </div>
    </fieldset>
</form>
