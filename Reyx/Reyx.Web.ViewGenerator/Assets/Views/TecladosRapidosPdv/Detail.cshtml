@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/tecladosrapidospdv.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Teclados Rapidos Pdv</legend>
    @Html.Hidden("ID_TRPD")
        <div class="control-group">
            <label class="control-label" for="ID_PDV">Id Pdv</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_PDV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_TCRP">Id Tcrp</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_TCRP)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ORDEM_TRPD">Ordem Trpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.ORDEM_TRPD)
            </div>
        </div>
    </fieldset>
</form>
