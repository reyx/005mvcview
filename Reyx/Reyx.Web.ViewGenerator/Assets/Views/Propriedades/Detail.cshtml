@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/propriedades.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Propriedades</legend>
    @Html.Hidden("COD_PROP")
        <div class="control-group">
            <label class="control-label" for="DESC_PROP">Desc Prop</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESC_PROP)
            </div>
        </div>
    </fieldset>
</form>
