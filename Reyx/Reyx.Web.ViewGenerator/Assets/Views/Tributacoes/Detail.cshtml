@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/tributacoes.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Tributacoes</legend>
    @Html.Hidden("ID_TRIB")
        <div class="control-group">
            <label class="control-label" for="COD_TRIB">Cod Trib</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_TRIB)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="TIPO_TRIB">Tipo Trib</label>
            <div class="controls">
                @Html.EditorFor(model => model.TIPO_TRIB)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DESC_TRIB">Desc Trib</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESC_TRIB)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBS_TRIB">Obs Trib</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBS_TRIB)
            </div>
        </div>
    </fieldset>
</form>
