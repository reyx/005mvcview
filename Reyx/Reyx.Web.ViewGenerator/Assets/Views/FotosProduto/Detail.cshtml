@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/fotosproduto.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Fotos Produto</legend>
    @Html.Hidden("ID_FTPD")
        <div class="control-group">
            <label class="control-label" for="COD_PROD">Cod Prod</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_PROD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DESC_FTPD">Desc Ftpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESC_FTPD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="FOTO_FTPD">Foto Ftpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.FOTO_FTPD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="PRINCIPAL_FTPD">Principal Ftpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.PRINCIPAL_FTPD)
            </div>
        </div>
    </fieldset>
</form>
