        <div class="control-group">
            <label class="control-label" for="FOTO_PESSOA">Foto Pessoa</label>
            <div class="controls">
                @Html.EditorFor(model => model.FOTO_PESSOA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="SENHA_PDV">Senha Pdv</label>
            <div class="controls">
                @Html.EditorFor(model => model.SENHA_PDV)
