@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/pessoas.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
