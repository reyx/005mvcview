@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/contatoscliente.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Contatos Cliente</legend>
    @Html.Hidden("COD_CTCL")
        <div class="control-group">
            <label class="control-label" for="TIPO_CTCL">Tipo Ctcl</label>
            <div class="controls">
                @Html.EditorFor(model => model.TIPO_CTCL)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATA_CTCL">Data Ctcl</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATA_CTCL)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBS_CTCL">Obs Ctcl</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBS_CTCL)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_CLI">Cod Cli</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_CLI)
            </div>
        </div>
    </fieldset>
</form>
