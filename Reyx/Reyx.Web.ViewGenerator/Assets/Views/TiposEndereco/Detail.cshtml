@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/tiposendereco.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Tipos Endereco</legend>
    @Html.Hidden("ID_TPEN")
        <div class="control-group">
            <label class="control-label" for="DESCR_TPEN">Descr Tpen</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESCR_TPEN)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="IMAGEM_TPEN">Imagem Tpen</label>
            <div class="controls">
                @Html.EditorFor(model => model.IMAGEM_TPEN)
            </div>
        </div>
    </fieldset>
</form>
