@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/consultoreslojas.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Consultores Lojas</legend>
    @Html.Hidden("ID_CNLJ")
        <div class="control-group">
            <label class="control-label" for="COD_CONS">Cod Cons</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_CONS)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
    </fieldset>
</form>
