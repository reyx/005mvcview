@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/inventario.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Inventario</legend>
    @Html.Hidden("ID_INV")
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATINI_INV">Datini Inv</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATINI_INV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATFIN_INV">Datfin Inv</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATFIN_INV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="VALOREST_INV">Valorest Inv</label>
            <div class="controls">
                @Html.EditorFor(model => model.VALOREST_INV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="VALORINV_INV">Valorinv Inv</label>
            <div class="controls">
                @Html.EditorFor(model => model.VALORINV_INV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_OPER">Id Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LCES">Cod Lces</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LCES)
            </div>
        </div>
    </fieldset>
</form>
