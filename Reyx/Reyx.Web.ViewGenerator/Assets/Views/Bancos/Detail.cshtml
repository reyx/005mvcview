@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/bancos.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Bancos</legend>
        <div class="control-group">
            <label class="control-label" for="COD_BCO">Cod Bco</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_BCO)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="NOME_BCO">Nome Bco</label>
            <div class="controls">
                @Html.EditorFor(model => model.NOME_BCO)
            </div>
        </div>
    </fieldset>
</form>
