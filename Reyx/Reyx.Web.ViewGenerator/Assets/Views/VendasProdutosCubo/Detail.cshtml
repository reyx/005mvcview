@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/vendasprodutoscubo.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Vendas Produtos Cubo</legend>
    @Html.Hidden("ID_VDPC")
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_VLPR">Id Vlpr</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_VLPR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="QTDE_ITVD">Qtde Itvd</label>
            <div class="controls">
                @Html.EditorFor(model => model.QTDE_ITVD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="PRECO_ITVD">Preco Itvd</label>
            <div class="controls">
                @Html.EditorFor(model => model.PRECO_ITVD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="CUSTO_PROD">Custo Prod</label>
            <div class="controls">
                @Html.EditorFor(model => model.CUSTO_PROD)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATA_VDA">Data Vda</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATA_VDA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DEVOLUCAO_ITVD">Devolucao Itvd</label>
            <div class="controls">
                @Html.EditorFor(model => model.DEVOLUCAO_ITVD)
            </div>
        </div>
    </fieldset>
</form>
