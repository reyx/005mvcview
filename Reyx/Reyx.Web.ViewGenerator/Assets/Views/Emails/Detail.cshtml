@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/emails.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Emails</legend>
    @Html.Hidden("ID_EMAIL")
        <div class="control-group">
            <label class="control-label" for="ENDERECO_EMAIL">Endereco Email</label>
            <div class="controls">
                @Html.EditorFor(model => model.ENDERECO_EMAIL)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBS_EMAIL">Obs Email</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBS_EMAIL)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_PESSOA">Id Pessoa</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_PESSOA)
            </div>
        </div>
    </fieldset>
</form>
