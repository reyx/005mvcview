@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/agencias.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Agencias</legend>
    @Html.Hidden("ID_AGEN")
        <div class="control-group">
            <label class="control-label" for="NUMERO_AGEN">Numero Agen</label>
            <div class="controls">
                @Html.EditorFor(model => model.NUMERO_AGEN)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DIGITO_AGEN">Digito Agen</label>
            <div class="controls">
                @Html.EditorFor(model => model.DIGITO_AGEN)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="NOME_AGEN">Nome Agen</label>
            <div class="controls">
                @Html.EditorFor(model => model.NOME_AGEN)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBS_AGEN">Obs Agen</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBS_AGEN)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_BCO">Cod Bco</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_BCO)
            </div>
        </div>
    </fieldset>
</form>
