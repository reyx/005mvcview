@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/enderecospessoatiposendereco.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Enderecos Pessoa Tipos Endereco</legend>
    @Html.Hidden("ID_ENTP")
        <div class="control-group">
            <label class="control-label" for="ID_ENPS">Id Enps</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_ENPS)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="TIPO_ENTP">Tipo Entp</label>
            <div class="controls">
                @Html.EditorFor(model => model.TIPO_ENTP)
            </div>
        </div>
    </fieldset>
</form>
