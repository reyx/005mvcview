@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/movimentacoesequipamento.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Movimentacoes Equipamento</legend>
    @Html.Hidden("ID_MVEQ")
        <div class="control-group">
            <label class="control-label" for="ID_EQUIP">Id Equip</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_EQUIP)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_OPER">Id Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_CLI">Cod Cli</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_CLI)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_HSEQ">Id Hseq</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_HSEQ)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DATA_MVEQ">Data Mveq</label>
            <div class="controls">
                @Html.EditorFor(model => model.DATA_MVEQ)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DOCTO_MVEQ">Docto Mveq</label>
            <div class="controls">
                @Html.EditorFor(model => model.DOCTO_MVEQ)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="OBS_MVEQ">Obs Mveq</label>
            <div class="controls">
                @Html.EditorFor(model => model.OBS_MVEQ)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="PREV_RETORNO_MVEQ">Prev Retorno Mveq</label>
            <div class="controls">
                @Html.EditorFor(model => model.PREV_RETORNO_MVEQ)
            </div>
        </div>
    </fieldset>
</form>
