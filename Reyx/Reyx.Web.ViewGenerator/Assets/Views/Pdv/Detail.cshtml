@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/pdv.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Pdv</legend>
    @Html.Hidden("ID_PDV")
        <div class="control-group">
            <label class="control-label" for="NUMERO_PDV">Numero Pdv</label>
            <div class="controls">
                @Html.EditorFor(model => model.NUMERO_PDV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="DESCRICAO_PDV">Descricao Pdv</label>
            <div class="controls">
                @Html.EditorFor(model => model.DESCRICAO_PDV)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_ECF">Id Ecf</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_ECF)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LISTA">Cod Lista</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LISTA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_GRPD">Cod Grpd</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_GRPD)
            </div>
        </div>
    </fieldset>
</form>
