@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/funcoesoperador.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Funcoes Operador</legend>
    @Html.Hidden("ID_FNOP")
        <div class="control-group">
            <label class="control-label" for="ID_OPER">Id Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_FNPR">Id Fnpr</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_FNPR)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="PERMITIDO_FNOP">Permitido Fnop</label>
            <div class="controls">
                @Html.EditorFor(model => model.PERMITIDO_FNOP)
            </div>
        </div>
    </fieldset>
</form>
