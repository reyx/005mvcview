@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/lojasoperador.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Lojas Operador</legend>
    @Html.Hidden("ID_LJOP")
        <div class="control-group">
            <label class="control-label" for="ID_OPER">Id Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="COD_LOJA">Cod Loja</label>
            <div class="controls">
                @Html.EditorFor(model => model.COD_LOJA)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="NIVELLOJA_OPER">Nivelloja Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.NIVELLOJA_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="NIVELPDV_LJOP">Nivelpdv Ljop</label>
            <div class="controls">
                @Html.EditorFor(model => model.NIVELPDV_LJOP)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="NIVELRET_LJOP">Nivelret Ljop</label>
            <div class="controls">
                @Html.EditorFor(model => model.NIVELRET_LJOP)
            </div>
        </div>
    </fieldset>
</form>
