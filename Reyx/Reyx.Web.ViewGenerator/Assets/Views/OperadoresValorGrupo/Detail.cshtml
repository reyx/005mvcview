@{
    Layout = "~/Views/Shared/_Pages.cshtml";
}
@session Scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/cadastros/operadoresvalorgrupo.js")"></script>
}
<form action="/" method="post" class="form-horizontal">
    <fieldset>
        <legend>Operadores Valor Grupo</legend>
    @Html.Hidden("ID_OPVG")
        <div class="control-group">
            <label class="control-label" for="ID_OPER">Id Oper</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_OPER)
            </div>
        </div>
        <div class="control-group">
            <label class="control-label" for="ID_VLGR">Id Vlgr</label>
            <div class="controls">
                @Html.EditorFor(model => model.ID_VLGR)
            </div>
        </div>
    </fieldset>
</form>
